<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>cblink.dubbo</groupId>
    <artifactId>cblink.dubbo</artifactId>
    <version>1.0.0</version>
  </parent>
  <groupId>cblink.dubbo.provider</groupId>
  <artifactId>cblink.dubbo.provider</artifactId>
  <packaging>war</packaging>
  <name>cblink.dubbo.provider Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
    	 <groupId>cblink.dubbo.common</groupId>
    	 <artifactId>cblink.dubbo.common</artifactId>
    	 <version>1.0.0</version>
    </dependency>
  </dependencies>
  <build>
    <finalName>cblink.dubbo.provider</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.5.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
